
# Collection Operations
# Insertion , Deletion , Update , Length , Sort , Search

let array : {1,2,3,4,5};

# Print Array to screen
log array;

# Accessing Collection Elemenets
log array[0] , array[-1]

# Manipulating Collection Elemenets
array[2] : 2000
log array

# Collection Length
log "Length : " ,  length{array}

# Delete A Elemenet From Collection
let value : pop array[0]
log "Pop value : " , value , array

# Reverse a Collection
log reverse{array}

# Insert value at First in Collection 
log { 500 } + array 

# Insert value at Last in Collection
log array + { 30 }

# Remove a element from specific index

log array
log remove{array , -1}
log array


# Insert a element at specific index

log array , length{array}
log insert{array , 1 , 100}
log insert{array , 3 , 500} 
log array , length{array}









